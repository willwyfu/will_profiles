#!/bin/sh

oamount
if [ -z $1 ] | [ -z $2 ] | [ -z $3 ]; then
	echo "rcp project_code dbg/rel file"
	echo "    Code: n61003"
	exit 0
fi

TAGET_BASE=~/OaTemp/temp/Will

if [ "$1" = "n61003" ]; then
	if [ "$2" = "dbg" ]; then
		PROJECT=N61003_NAND_LA_BSP_dbg
	fi

	if [ "$2" = "rel" ]; then
		PROJECT=N61003_NAND_LA_BSP_rel
	fi
fi

if [ "$1" = "n33004" ]; then
	if [ "$2" = "dbg" ]; then
		PROJECT=N33004_NAND_EU_BSP_dbg
	fi

	if [ "$2" = "rel" ]; then
		PROJECT=N33004_NAND_EU_BSP_rel
	fi
fi

if [ -z ${PROJECT} ]; then
	echo "Error args"
	exit 1
fi



shift
shift
echo $@
TARGET=${TAGET_BASE}/${PROJECT}
mkdir -p ${TARGET}
echo Copy "$@" to ${TARGET}
cp $@ ${TARGET}/
